{
    "name": "jobmetric/laravel-unit",
    "description": "This is a unit management package for Laravel that you can use in your projects.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "unit",
        "management"
    ],
    "homepage": "https://doc.jobmetric.net/package/laravel-unit",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.7",
        "jobmetric/laravel-translation": "^2.4"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\Unit\\": "src/",
            "JobMetric\\Unit\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\Unit\\UnitServiceProvider"
            ],
            "aliases": {
                "Unit": "JobMetric\\Unit\\Facades\\Unit"
            }
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-unit/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-unit"
    }
}
